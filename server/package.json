{
  "name": "remote-dev-jobs-streamer",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "development": "NODE_ENV=development concurrently --kill-others \"npm run client\" \"npm run server\"",
    "production": "npm run build && NODE_ENV=production npm run server",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "bash ./start-all.sh",
    "start-server": "node server.js",
    "start-client": "cd client && react-scripts start",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "http": "0.0.0",
    "http-proxy-middleware": "^1.0.3",
    "path": "^0.12.7",
    "request": "^2.88.0",
    "socket.io": "^2.3.0",
    "util": "^0.12.1"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  },
  "engine": {
    "node": "12.x"
  }
}
